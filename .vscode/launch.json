{
    "version": "0.2.0",
    "configurations": [      
      {
        "name": "Debug Current File",
        "type": "debugpy",
        "request": "launch",
        "program": "${file}",
        "console": "integratedTerminal"
      },
      {
        "name": "Profile Python Code",
        "type": "debugpy",
        "request": "launch",
        "program": "${file}",
        "console": "integratedTerminal",
        "justMyCode": true,
        "args": [
            "-m",
            "cProfile",
            "-o",
            "profile_results.prof"
        ]
    }
    ]
  }
  